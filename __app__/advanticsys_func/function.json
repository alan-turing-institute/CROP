{
  "scriptFile": "__init__.py",
  "entryPoint": "advanticsys_import",
  "bindings": [
    {
        "name": "blobin",
        "type": "blobTrigger",
        "direction": "in",
        "path": "advanticsys-raw-data/{blobname}.{blobextension}",
        "connection": "AzureWebJobsStorage"
    },
    {
        "name": "blobout",
        "type": "blob",
        "direction": "out",
        "path": "advanticsys-processed-data/{blobname}.{blobextension}",
        "connection": "AzureWebJobsStorage"
    }
  ]
}

    